{
  "name": "mayako-remote",
  "version": "0.2.7",
  "description": "远程模块加载器",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "bin": {
    "mayako-remote": "sh.js"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "echo Error: missing tests",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "lint": "eslint -c .eslintrc.js 'src/**/*.js'",
    "lint:fix": "eslint --fix -c .eslintrc.js 'src/**/*.js' --fix",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build": "npm run clean && npm run build:self && npm run build:esm"
  },
  "author": "mayako21126",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mayako21126/mayako-remote.git"
  },
  "bugs": {
    "url": "https://github.com/mayako21126/mayako-remote/issues"
  },
  "files": [
    "/dist",
    "*.d.ts",
    "sh.js",
    "setting.js",
    "/src"
  ],
  "engines": {
    "node": ">= 0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.1.0",
    "@js-lib/cli": "^1.6.0",
    "axios": "^0.20.0",
    "cross-env": "5.2.0",
    "es5-shim": "4.5.10",
    "escodegen": "^2.0.0",
    "eslint": "^5.15.1",
    "esprima": "^4.0.1",
    "estraverse": "^5.3.0",
    "eval-estree-expression": "^1.1.0",
    "rimraf": "2.6.2",
    "rollup": "0.57.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-uglify": "1.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "axios": "^0.20.0",
    "co": "^4.6.0",
    "escodegen": "^2.0.0",
    "esprima": "^4.0.1",
    "estraverse": "^5.3.0",
    "eval-estree-expression": "^1.1.0",
    "qs": "^6.10.3"
  }
}
